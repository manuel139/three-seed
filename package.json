{
  "name": "pixi-seed",
  "version": "0.1.4",
  "repository": "https://github.com/edwinwebb/pixi-seed",
  "description": "Pixi.js project seed with ES6 and webpack",
  "config": {
    "buildDir": "./build",
    "devPort": 8080,
    "devHost": "localhost"
  },
  "scripts" : {
    "prestart": "npm install",
    "start" : "webpack-dev-server --content-base ./build/ --hot",
    "webpack" : "webpack -p --config ./webpack.config.js",
    "prewebpack" : "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir"
  },
  "dependencies": {
    "pixi.js": "2.2.9",
    "flux": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^5.1.11",
    "babel-eslint": "^1.0.12",
    "babel-loader": "^5.0.0",
    "json-loader": "^0.5.1",
    "babel-runtime": "^5.1.11",
    "file-loader": "^0.8.1",
    "html-loader": "^0.2.3",
    "lodash": "^3.3.1",
    "tween.js": "^0.14.0",
    "webpack": "^1.9.5",
    "webpack-dev-server": "^1.8.2",
    "rimraf": "^2.3.2"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
