{
  "name": "three-seed",
  "version": "0.1.0",
  "repository": "https://github.com/edwinwebb/three-seed",
  "description": "Three.js project seed with ES6 and webpack",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --hot --inline",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production npm run webpack",
    "clean": "rimraf ./build && mkdir ./build",
    "lint": "eslint ./app/**/*.js",
    "reactman": "reactman --config ./templates/reactman.config.js"
  },
  "config": {
    "title": "Three.js Project",
    "build": "./build/"
  },
  "dependencies": {
    "three": "^0.76.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.10.2",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "json-loader": "^0.5.3",
    "lodash": "^4.13.1",
    "reactman": "^3.2.0",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
