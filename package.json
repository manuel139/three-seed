{
  "name": "three-seed",
  "version": "0.1.0",
  "repository": "https://github.com/edwinwebb/three-seed",
  "description": "Three.js project seed with ES6 and webpack",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --hot --inline",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production npm run webpack",
    "clean": "rimraf ./build && mkdir ./build",
    "lint": "eslint ./app/**/*.js",
    "reactman": "reactman --config ./templates/reactman.config.js"
  },
  "config": {
    "title": "Three.js Project",
    "build": "./build/"
  },
  "dependencies": {
    "gsap": "^1.18.5",
    "three": "^0.82.0",
    "three-orbit-controls": "^72.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "lodash": "^4.13.1",
    "reactman": "^3.5.2",
    "rimraf": "^2.5.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
