{
  "name": "pixi-seed",
  "version": "0.1.4",
  "repository": "https://github.com/edwinwebb/pixi-seed",
  "description": "Pixi.js project seed with ES6 and webpack",
  "config": {
    "buildDir": "./build",
    "devPort": 8080,
    "devHost": "localhost"
  },
  "scripts" : {
    "prestart": "npm install",
    "start" : "webpack-dev-server --content-base ./build/ --hot",
    "webpack" : "webpack -p --config ./webpack.config.js",
    "prewebpack" : "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir"
  },
  "dependencies": {
    "pixi.js": "3.0.7",
    "flux": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "json-loader": "^0.5.2",
    "babel-runtime": "^5.8.20",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "lodash": "^3.10.1",
    "tween.js": "^15.0.0",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1",
    "rimraf": "^2.4.3"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
